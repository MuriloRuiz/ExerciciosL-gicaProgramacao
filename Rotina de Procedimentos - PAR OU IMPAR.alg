algoritmo "semnome"
// Função :
// Autor :
// Data : 18/01/2022
// Seção de Declarações 
var
   n : inteiro
procedimento parouimpar (v : inteiro )
inicio
      se ( n%2 = 0) entao
      escreval (" seu numero é par ")
      senao
      escreval (" seu numero é impar")
      fimse

fimprocedimento

inicio
      escreval (" escreva um numero : ")
      leia (n)
      parouimpar (n)
fimalgoritmo
